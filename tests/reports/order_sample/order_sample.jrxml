	<queryString>
		<![CDATA[SELECT
     base_customer.`id` AS base_customer_id,
     base_customer.`name` AS base_customer_name,
     base_customer.`street` AS base_customer_street,
     base_customer.`zipcode` AS base_customer_zipcode,
     base_customer.`city` AS base_customer_city,
     base_customer.`country_id` AS base_customer_country_id,
     base_country.`iso_3166_1` AS base_country_iso_3166_1,
     sal_shopcart.`id` AS sal_shopcart_id,
     sal_shopcart.`comment` AS sal_shopcart_comment,
     sal_shopcart_status_translation.`title` AS sal_shopcart_status_translation_title,
     base_pricelist.`name` AS base_pricelist_name,
     sal_shopcart_line.`quantity` AS quantity,
     sal_shopcart_line.`quantity`*base_product_price.`price_sale` AS price_total_sale_net,
     base_product_price.`price_sale` AS base_product_price_price_sale,
     base_product.`volume` AS volume,
     base_product.`weight` AS weight,
     base_product_brand.`name` AS base_product_brand_name,
     base_product_translation.`invoice_title` AS base_product_translation_invoice_title,
     base_product_translation.`commercial_title` AS base_product_translation_commercial_title,
     base_product_translation.`commercial_characteristic` AS base_product_translation_commercial_characteristic,
     base_product.`reference` AS base_product_reference,
     sal_shopcart.`created_at` AS sal_shopcart_created_at,
     sal_shopcart.`updated_at` AS sal_shopcart_updated_at,
     auth_user.`username` AS auth_user_username,
     sal_shopcart.`status_id` AS sal_shopcart_status_id,
     sal_shopcart_line.`product_id` AS sal_shopcart_line_product_id
FROM
     `sal_shopcart` sal_shopcart INNER JOIN `base_customer` base_customer ON sal_shopcart.`customer_id` = base_customer.`id`
     INNER JOIN `base_country` base_country ON base_customer.`country_id` = base_country.`id`
     INNER JOIN `auth_user` auth_user ON sal_shopcart.`user_id` = auth_user.`id`
     INNER JOIN `sal_shopcart_status` sal_shopcart_status ON sal_shopcart.`status_id` = sal_shopcart_status.`id`
     INNER JOIN `base_pricelist` base_pricelist ON sal_shopcart.`pricelist_id` = base_pricelist.`id`
     INNER JOIN `sal_shopcart_line` sal_shopcart_line ON sal_shopcart.`id` = sal_shopcart_line.`shopcart_id`
     INNER JOIN `base_product` base_product ON sal_shopcart_line.`product_id` = base_product.`id`
     INNER JOIN `base_product_price` base_product_price ON base_product.`id` = base_product_price.`product_id`
     INNER JOIN `base_product_brand` base_product_brand ON base_product.`brand_id` = base_product_brand.`id`
     INNER JOIN `base_product_translation` base_product_translation ON base_product.`id` = base_product_translation.`id`
     LEFT OUTER JOIN `sal_shopcart_status_translation` sal_shopcart_status_translation ON sal_shopcart_status.`id` = sal_shopcart_status_translation.`id`
WHERE
     sal_shopcart.`id` = $P{SHOPCART_ID}
 AND sal_shopcart_status_translation.`lang` = $P{LANG}
 AND base_product_translation.`lang` = $P{LANG}
 AND base_product_price.`pricelist_id` = sal_shopcart.`pricelist_id`]]>
	</queryString>
