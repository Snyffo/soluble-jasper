{
    "name": "soluble/jasper",
    "description": "Jasper reports in PHP",
    "license": "MIT",
    "keywords": [
        "jasper",
        "jasperreports",
        "reports",
        "pdf",
        "phpjavabridge",
        "japha"
    ],
    "homepage": "http://soluble.io",
    "type": "library",
    "authors": [
        {
            "name": "SÃ©bastien Vanvelthem",
            "homepage": "https://github.com/belgattitude"
        }
    ],
    "autoload": {
        "psr-4": {
            "Soluble\\Jasper\\": "src/Soluble/Jasper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JasperTest\\": "tests/JasperTest"
        }
    },
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "soluble/japha": "^2.2.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9",
        "friendsofphp/php-cs-fixer": "^2.4.0",
        "monolog/monolog": "^1.21.0",
        "phpstan/phpstan": "^0.8.2",
        "roave/security-advisories": "dev-master",
        "pds/skeleton": "@dev"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@phpstan"
        ],
        "fix": [
            "@cs-fix"
        ],
        "test": "vendor/bin/phpunit",
        "cs-check": "vendor/bin/php-cs-fixer --diff --dry-run -v fix",
        "cs-fix": "vendor/bin/php-cs-fixer -v fix",
        "phpstan": "vendor/bin/phpstan analyse -l max -c phpstan.neon src",
        "phpds": "vendor/bin/pds-skeleton validate",
        "build-doc": "mkdocs build"
    },
    "suggest": {
        "monolog/monolog": "PSR-3 comptatible logger"
    },
    "archive": {
        "exclude": [".sami.php", "phpstan.neon", "test", "doc", ".travis", ".travis.yml", ".codeclimate.yml", ".coveralls.yml", ".scrutinizer.yml", ".php_cs", ".gitignore", "phpcs.xml"]
    }
}
